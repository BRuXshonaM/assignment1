#task1
def kwargsAcceptFun(**kwargs):
    """Function that accepts any number of keyword arguments and prints them."""
    for key, value in kwargs.items():
        print(f"{key}: {value}")

# Example usage
kwargsAcceptFun(name="Alice", age=25, city="Wonderland")
kwargsAcceptFun(course="Python", level="Intermediate", duration="3 months")
